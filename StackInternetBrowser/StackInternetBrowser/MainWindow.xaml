<RibbonWindow x:Class="Jiuyong.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jiuyong"
        mc:Ignorable="d"
        Title="MainWindow" ResizeMode="CanResizeWithGrip" WindowState="Maximized">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>
		<Ribbon Grid.Row="0" IsMinimized="True"/>
		<WebBrowser Grid.Row="1"/>
		<StatusBar Grid.Row="9" Padding="0,0,20,0">
			<StatusBarItem>状态栏</StatusBarItem>
		</StatusBar>
	</Grid>
</RibbonWindow>
